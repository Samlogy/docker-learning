generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int       @id @default(autoincrement())
  businessName String
  logo         String
  email        String    @unique
  fullName     String
  address      String
  website      String?
  phone        String
  noteClient   String?
  signature    String
  bankAccount  String
  payments     String
  createdAt    DateTime  @default(now())
  editedAt     DateTime?
}
